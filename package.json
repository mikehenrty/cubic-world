{
  "name": "cubic-world",
  "type": "module",
  "version": "0.1.0",
  "license": "CC-BY-3.0",
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "serve": "yarn clean && parcel index.html --log-level verbose",
    "build": "yarn clean && parcel build index.html --public-url ./ --no-scope-hoist",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "parcel": "^2.0.0-alpha.3.2"
  },
  "dependencies": {
    "three": "^0.113.2"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
